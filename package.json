{
  "name": "cal",
  "version": "1.0.0",
  "description": "Calendar scraper for Aspuddens IP",
  "main": "dist/cli/generate-ics.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli/generate-ics.js",
    "scrape:venue": "node dist/scrapers/venue-scraper.js",
    "scrape:team": "node dist/scrapers/team-scraper.js",
    "scrape:laget": "node dist/scrapers/laget-scraper.js",
    "dev": "ts-node-dev src/cli/generate-ics.ts",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\""
  },
  "dependencies": {
    "ics": "^3.5.0",
    "jsdom": "^21.1.0",
    "node-ical": "^0.20.1"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.0",
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2",
    "prettier": "^2.8.8"
  }
}
