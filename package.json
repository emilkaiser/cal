{
  "name": "cal",
  "version": "1.0.0",
  "description": "Calendar scraper for Aspuddens IP",
  "main": "dist/cli/generate-ics.js",
  "scripts": {
    "build": "npm run build:node && next build",
    "build:node": "npm run clean && tsc --project tsconfig.node.json",
    "build:ts": "tsc --project tsconfig.node.json",
    "start": "next start",
    "scrape:venue": "node dist/scrapers/venue-scraper.js",
    "scrape:team": "node dist/scrapers/team-scraper.js",
    "scrape:laget": "node dist/scrapers/laget-scraper.js",
    "dev": "next dev -p 3003",
    "dev:cli": "ts-node-dev --project tsconfig.node.json src/cli/generate-ics.ts",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "debug:calendar": "ts-node --project tsconfig.node.json src/cli/debug-calendar.ts",
    "debug:venue": "ts-node --project tsconfig.node.json src/cli/debug-venue.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toggle": "^1.1.2",
    "@radix-ui/react-toggle-group": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@tailwindcss/postcss": "^4.0.15",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^2.30.0",
    "ics": "^3.5.0",
    "jsdom": "^21.1.0",
    "lucide-react": "^0.483.0",
    "moment": "^2.29.4",
    "next": "^14.0.4",
    "next-themes": "^0.4.6",
    "node-ical": "^0.20.1",
    "postcss": "^8.5.3",
    "react": "^18.2.0",
    "react-big-calendar": "^1.8.5",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.15",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.2.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-big-calendar": "^1.8.5",
    "@types/react-dom": "^18.2.18",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
